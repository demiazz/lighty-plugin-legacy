{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "no-param-reassign": 0,
    "prefer-arrow-callback": 0,
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true,
      "optionalDependencies": false,
      "peerDependencies": false,
    }]
  },
  "globals": {
    "describe": false,
    "context": false,
    "beforeAll": false,
    "afterAll": false,
    "beforeEach": false,
    "afterEach": false,
    "it": false,
    "expect": false,
    "sinon": false
  }
}
